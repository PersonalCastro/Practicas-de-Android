<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorWite</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>



        <item name="android:icon">@color/colorAccent</item>

        <item name="android:itemBackground">@color/colorAccent</item>

        <item name="android:textAllCaps">false</item>


        <item name="colorControlNormal">@color/colorRatingBar0_5</item>
        <item name="colorControlActivated">@color/colorRatingBar</item>


    </style>

    <style name="Empty">

    </style>

    <style name="RatingBar"
        parent="android:style/Widget.Material.RatingBar.Small">

    </style>



</resources>
